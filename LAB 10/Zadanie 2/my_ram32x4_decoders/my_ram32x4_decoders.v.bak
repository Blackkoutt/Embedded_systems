module my_ram32x4_decoders(input [4:0] address,
									input clk,	
									input [3:0] data,
									input wren,
									output [0:6] H5, H4, H2, H0);
									
	wire [3:0] q;
	
	my_ram32x4 ram(address, clk, data, wren, q);
	
	decoder_hex_16 dec_addr1({3'b000, address[4]}, H5[0:6]);
	decoder_hex_16 dec_addr0(address[3:0], H4[0:6]);
	decoder_hex_16 dec_data_in(data[3:0], H2[0:6]);
	decoder_hex_16 dec_out(q[3:0], H0[0:6]);
	
endmodule

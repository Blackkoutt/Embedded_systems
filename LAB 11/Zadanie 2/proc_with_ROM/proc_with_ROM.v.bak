module proc_with_ROM(input MClock, PClock, Resetn, Run,
							output [8:0] Bus,
							output Done);
							
	wire [4:0] address;
	wire [8:0] data;
	
	counter_N_bits #(5) counter(MClock, Resetn, address);
	rom32x9 ROM(address, MClock, data);
	proc processor(data, Resetn, PClock, Run, Done, Bus);
	
endmodule
	